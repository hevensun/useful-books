set fileencodings=utf-8,gbk,gb2312,cp936,chinese
set fileencoding=utf-8
set encoding=utf-8
set termencoding=utf-8
syntax on
set nocompatible
set backspace=indent,eol,start
set nobackup            "not keep backup file, use versions instead
set history=50          " keep 50 lines of command line history
set ruler               " show the cursor position all the time
set showcmd             " display incomplete commands
set incsearch           " do incremental searching
filetype plugin indent on
set shiftwidth=4
set autoindent
set ts=4
colorscheme evening
set ru
set tabstop=4
set enc=utf-8
set nocompatible
set smartindent
set cindent
set nobackup
set backupcopy=yes
set hls
